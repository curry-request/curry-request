{
  "name": "curry-request",
  "version": "1.1.0",
  "description": "Http request client built with one curried function",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --fix",
    "test": "mocha -r ts-node/register test/*.ts",
    "build-mjs": "tsc -d && mv lib/index.js lib/index.mjs && cp lib/index.mjs lib/index.es.js",
    "build-cjs": "tsc -m commonjs",
    "build": "rimraf ./lib && npm run build-mjs && npm run build-cjs",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "site-dev": "snowpack dev"
  },
  "author": "friki",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "isomorphic-unfetch": "^3.0.0",
    "mocha": "^8.0.1",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "snowpack": "^3.8.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@codemirror/commands": "^0.18.3",
    "@codemirror/gutter": "^0.18.4",
    "@codemirror/highlight": "^0.18.4",
    "@codemirror/history": "^0.18.1",
    "@codemirror/lang-javascript": "^0.18.0",
    "@codemirror/state": "^0.18.7",
    "@codemirror/view": "^0.18.19",
    "abort-controller": "^3.0.0",
    "cross-fetch": "^3.0.4",
    "left-pad": "^1.3.0",
    "modern-normalize": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fracalo/curry-request.git"
  },
  "keywords": [
    "http",
    "request",
    "fetch",
    "client"
  ]
}
