{
  "name": "curry-request",
  "version": "2.0.0",
  "description": "Http request client built with one curried function",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --fix",
    "test": "mocha -r ts-node/esm test/*.ts",
    "build-mjs": "tsc -m es6 -d && mv lib/index.js lib/index.mjs && cp lib/index.mjs lib/index.es.js",
    "build-cjs": "tsc",
    "build": "rimraf ./lib && npm run build-mjs && npm run build-cjs",
    "compile-md-examples": "node ../curry-request-docs/mdParsingScript.mjs",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "report-coverage": "nyc --reporter text-lcov npm test > coverage/coverage-final.json"
  },
  "author": "friki",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/estree": "^0.0.51",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "chai": "^4.3.6",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "isomorphic-unfetch": "^3.0.0",
    "mocha": "^10.0.0",
    "node-fetch": "^2.6.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.1",
    "abort-controller": "^3.0.0",
    "cross-fetch": "^3.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fracalo/curry-request.git"
  },
  "keywords": [
    "http",
    "request",
    "fetch",
    "client"
  ]
}
